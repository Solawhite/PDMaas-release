  "build": {
    "publish": {
      "provider": "generic",
      "url": "https://www.pdmaas.cn"
    },
    "appId": "PDMaas-CE",
    "productName": "PDMaas-CE",
    "artifactName": "${productName}-${os}_v${version}_${arch}.${ext}",
    "directories": {
      "buildResources": "app/build",
      "output": "dist"
    },
    "files": [
      "build/"
    ],
    "extends": null,
    "npmRebuild": false,
    "asar": true,
    "asarUnpack": [
      "build/service/",
      "build/jre/"
    ],
    "dmg": {
      "icon": "icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false
    },
    "linux": {
      "icon": "icons",
      "artifactName": "${productName}-${os}_${arch}_v${version}.${ext}",
      "target": {
        "target": "deb",
        "arch": [
          "arm64",
          "x64"
        ]
      }
    },
    "mac": {
      "artifactName": "${productName}-${os}_${arch}_v${version}.${ext}",
      "target": {
        "target": "dmg",
        "arch": [
          "arm64",
          "x64"
        ]
      }
    }
  }
}
